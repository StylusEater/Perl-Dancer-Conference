<specification>
  <list name="nav_day" iterator="nav_days">

  </list>
  <list name="talks" iterator="talks">
    <param name="panel-title" field="day"/>
    <param name="date" filter="date_only"/>
    <list name="scheduled-talk" iterator="talks">
      <param name="duration_display"/>
      <container name="session-location" value="room">
        <param name="location" field="room"/>
      </container>
      <param name="speaker-name" class="speaker-link" field="author.name"/>
      <param name="speaker-link" target="href" op="append" field="author.uri"/>
      <param name="photo-link" target="href" op="append" field="author.uri"/>
      <param name="photo" target="src" field="author.photo_uri"/>
      <param name="talk-title" class="talk-link" field="title"/>
      <param name="talk-link" target="href" op="append" field="seo_uri"/>
      <param name="short_abstract"/>
    </list>
  </list>
  <list name="unscheduled" iterator="unscheduled">
  </list>
</specification>
